{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prepare": "husky install",
    "code:lint": "eslint  \"**/*.{ts,tsx}\" --fix",
    "code:prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,css,md,json}\" --config .prettierrc.yml"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "reactstrap": "^8.9.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^11.0.1",
    "@types/react": "17.0.14",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "eslint": "7.30.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.1",
    "typescript": "4.3.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,md,json}": "prettier --write"
  }
}
